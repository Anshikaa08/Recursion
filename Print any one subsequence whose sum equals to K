#include<iostream>
#include<bits/stdc++.h>
#include<vector>
using namespace std;
void Print_subsequence(int i,vector<int>&ds,int k,int sum,int n,vector<int>&ans,int c){
    if(i>=n){
        if(sum==k && c>0){
            for(auto it:ds){
                cout<<it<<endl;
            }
            c--;
        }
        return;
    }
    ans.push_back(ds[i]);
    Print_subsequence(i+1,ds,k,sum+ds[i],n,ans,c);
    ans.pop_back();
    Print_subsequence(i+1,ds,k,sum-ds[i],n,ans,c);
}
int main(){
    int n,k,c;
    cin>>n>>k>>c;
    vector<int>ds;
    for(int i=0;i<n;i++){
        cin>>ds[i];
    }
    vector<int>ans;
    Print_subsequence(0,ds,k,0,n,ans,c);
    return 0;
}

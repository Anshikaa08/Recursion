#include<iostream>
#include<bits/stdc++.h>
#include<vector>
using namespace std;
int count_subsequence(int i,int k,vector<int>&arr,int s){
    if(i==arr.size()){
        if(s==k){
            return 1;
        }
        else return 0;
    }
    //s+=arr[i];
    int left=count_subsequence(i+1,k,arr,s+arr[i]);
    //s-=arr[i];
    int right=count_subsequence(i+1,k,arr,s);
    return left+right;
}
int main(){
    int n,k,s;
    cin>>n>>k;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    s=arr[0];
    cout<<count_subsequence(0,k,arr,s);
}
